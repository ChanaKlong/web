{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["node", "uuid"],
    "verbatimModuleSyntax": true,
    "plugins": [{ "name": "@astrojs/ts-plugin" }]
  }
}
